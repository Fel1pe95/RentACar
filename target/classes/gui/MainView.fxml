<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" stylesheets="@MainView.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainViewController">
   <children>
      <Pane prefHeight="500.0" prefWidth="200.0" style="-fx-background-color: #87CEFA;">
         <children>
            <Button fx:id="btCars" layoutX="25.0" layoutY="32.0" mnemonicParsing="false" onAction="#onBtCarsAction" prefHeight="30.0" prefWidth="150.0" text="Carros" />
            <Button fx:id="btRents" layoutX="25.0" layoutY="72.0" mnemonicParsing="false" onAction="#onBtRentsAction" prefHeight="30.0" prefWidth="150.0" text="Alugueis" />
         </children>
      </Pane>
      <VBox fx:id="vBox" layoutX="200.0" prefHeight="500.0" prefWidth="400.0" />
   </children>
</AnchorPane>
